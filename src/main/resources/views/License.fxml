        	<Button fx:id="prev" mnemonicParsing="false" text="Prev" />
        	<Button fx:id="next" mnemonicParsing="false" text="Next" />
        </buttons>
      </ButtonBar>
      <TextField fx:id="license" layoutX="102.0" layoutY="206.0" prefHeight="25.0" prefWidth="537.0" />
